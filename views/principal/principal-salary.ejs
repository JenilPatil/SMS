<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../includes/principal-head' , {title : 'Salary'})%>

    <link rel="stylesheet" href="/styles/principal/principlesalary.css">
</head>
<body>
    <div class="main">
        <%- include('../includes/principal-aside') %>
        <div class="main-content">
        <%- include('../includes/principal-header', { title: 'Your Salary' , subTitle: '' }) %>
        <div class="container">
            <div class="tablecontent">
                <table>
              <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Basic Salary</th>
                <th>A.G.P Salary</th>
                <th>D.A Salary</th>
                <th>H.R.A Salary</th>
                <th>Other Salary</th>
                <th>Total Salary</th>
              </tr>
              <tr>
                <td id="same">1</td>
                <td id="same"><%= staffSalary.name %></td>
                <td id="same"><%= staffSalary.salary.basic %></td>
                <td id="same"><%= staffSalary.salary.agp %></td>
                <td id="same"><%= staffSalary.salary.da %></td>
                <td id="same"><%= staffSalary.salary.hra %></td>
                <td id="same"><%= staffSalary.salary.other %></td>
                <td id="same"><%= staffSalary.salary.total %></td>
              </tr>
            </table>
            </div>
        </div>
    </div>
    
</body>
</html>
